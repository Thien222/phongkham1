{
  "name": "eye-clinic-server",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "prestart": "prisma db push --skip-generate",
    "start": "node src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name init",
    "db:studio": "prisma studio",
    "db:seed": "node src/scripts/seed.js",
    "db:reset": "prisma db push --force-reset && node src/scripts/seed.js",
    "backup": "node src/scripts/backup.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.6.1",
    "morgan": "^1.10.0",
    "@prisma/client": "^5.19.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.19.1",
    "nodemon": "^3.1.4"
  }
}


